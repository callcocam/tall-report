{
    "name": "callcocam/tall-report",
    "type": "package",
    "description": "The packages report table.",
    "keywords": ["framework", "laravel", "livewire", "alpinejs", "tailwindcss"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/jetstream": "^2.9",
        "livewire/livewire": "^2.5",
        "doctrine/dbal": "^3.3",
        "wireui/wireui": "^1.7",
        "callcocam/tall-schema": "dev-main",
        "callcocam/tall-sluggable": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Tall\\Report\\": "src"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Tall\\Report\\ReportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
